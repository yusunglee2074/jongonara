{
  "name": "jongonara",
  "version": "0.0.5",
  "description": "N카페 자동 등록기",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env NODE_ENV=development START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "precommit": "lint-staged",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder -p always",
    "postinstall": "electron-builder install-app-deps"
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{js,ts,tsx,json,css,scss,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "tslint --project ./tsconfig.json --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "build": {
    "productName": "jongonara",
    "appId": "org.yusunglee.jongonara",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "publish": {
        "provider": "github",
        "owner": "yusunglee2074",
        "private": true,
        "protocol": "https"
      }
    },
    "win": {
      "target": "nsis",
      "publish": {
        "provider": "github",
        "owner": "yusunglee2074",
        "private": true,
        "protocol": "https"
      }
    },
    "asar": true,
    "asarUnpack": [
      "node_modules/puppeteer/.local-chromium/**/*",
      "src/public/**/*",
      "dist/public/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yusunglee2074/jongonara"
  },
  "author": {
    "name": "yusunglee2074",
    "email": "lys0333@gmail.com"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/snnsnn/electron-react-typescript/issues"
  },
  "extraResources": "src/public/NES2/",
  "homepage": "https://github.com/snnsnn/electron-react-typescript",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/electron": "^1.6.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^23.3.12",
    "@types/puppeteer": "^2.0.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.0.3",
    "@types/styled-components": "^4.4.3",
    "@types/webdriverio": "^4.13.1",
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "css-url-relative-plugin": "^1.0.0",
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.6.3",
    "react-test-renderer": "^16.8.0-alpha.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@hot-loader/react-dom": "^16.11.0",
    "@types/date-fns": "^2.6.0",
    "@types/node-schedule": "^1.3.0",
    "antd": "^3.26.11",
    "axios": "^0.19.2",
    "date-fns": "^2.10.0",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.0",
    "fast-crc32c": "^2.0.0",
    "node-schedule": "^1.3.2",
    "postscribe": "^2.0.8",
    "puppeteer": "2.0.0",
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.1"
  }
}
